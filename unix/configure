  enableval="$enable_tk"
  TKX_BUILD=$enableval
else
  TKX_BUILD=YES
fi


case "$TKX_BUILD" in
    YES|yes)
        echo "$ac_t""Will build Tk specific code and wishx" 1>&6
        TKX_BUILD=YES
        ;;
    NO|no)
        echo "$ac_t""Will not build Tk specific code and wishx" 1>&6
        TKX_BUILD=NO
        ;;
     *)
        { echo "configure: error: Invalid argument to --enable-tk=" 1>&2; exit 1; }
        ;;
esac


