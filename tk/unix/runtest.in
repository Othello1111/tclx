#!/bin/sh

TCL_LIBRARY="@TCL_LIBRARY_DIR@"
TCLX_LIBRARY="@TCLX_RUNTIME_DIR@"
TK_LIBRARY="@TK_LIBRARY_DIR@"
TKX_LIBRARY="@TKX_RUNTIME_DIR@"
TCL_PROGRAM="@TCLX_BIN_DIR@"/tcl
TKTEST=@TKX_BIN_DIR@/tktest
export TCL_LIBRARY TCLX_LIBRARY TK_LIBRARY TKX_LIBRARY TCL_PROGRAM

LD_LIBRARY_PATH="@TKX_LIB_DIR@:@TCLX_LIB_DIR@:@TK_LIB_DIR@:@TCL_LIB_DIR@:${LD_LIBRARY_PATH}"
SHLIB_PATH="@TKX_LIB_DIR@:@TCLX_LIB_DIR@:@TK_LIB_DIR@:@TCL_LIB_DIR@:${SHLIB_PATH}"
export LD_LIBRARY_PATH SHLIB_PATH

exec $TKTEST ${1+"$@"}
