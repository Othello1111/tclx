all:
        cd ..\tcl\win
        nmake /f makefile.vc all
        cd ..\..\tk\win
        nmake /f makefile.vc all

static:
        cd ..\tcl\win
        nmake /f makefile.vc static
        cd ..\..\tk\win
        nmake /f makefile.vc static

static:
        cd ..\tcl\win
        nmake /f makefile.vc static
        cd ..\..\tk\win
        nmake /f makefile.vc static

standalone:
        cd ..\tcl\win
        nmake /f makefile.vc standalone
        cd ..\..\tk\win
        nmake /f makefile.vc standalone

test:
        cd ..\tcl\win
        nmake -f makefile.vc test

tktest:
        cd ..\tk\win
        nmake /f makefile.vc test

install:
        cd ..\tcl\win
        nmake /f makefile.vc install
        cd ..\..\tk\win
        nmake /f makefile.vc install
        
clean:
        cd ..\tcl\win
        nmake -f makefile.vc clean
        cd ..\..\tk\win
        nmake /f makefile.vc clean
        


